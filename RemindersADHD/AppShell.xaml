<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="RemindersADHD.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:RemindersADHD"
       xmlns:views="clr-namespace:RemindersADHD.MVVM.Views"
       Title="RemindersADHD"
       Shell.FlyoutBehavior="Flyout"
       Style="{StaticResource Shell}">

    <ShellContent x:Name="ListFlyoutItem"
                  Title="List View"
                  Route="listview"/>
    <ShellContent x:Name="TestFlyoutItem"
                  Title="Test view"
                  Route="testingshit" />
    <ShellContent x:Name="RoutinesFlyoutItem"
                  Title="Routines"
                  Route="routines" />
    <ShellContent x:Name="PomodoroFlyoutItem"
                  Title="Pomodoro"
                  Route="pomodoro" />
    <ShellContent x:Name="HabitsFlyoutItem"
                  Title="Habits"
                  Route="habits" />
    <ShellContent x:Name="ShoppingListFlyoutItem"
                  Title="Shopping List"
                  Route="shoppinglist" />
</Shell>
